{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "copyFactorySG"
		},
		"copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'copy'"
		},
		"source_to_DB_EMP_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source_to_DB_EMP'"
		},
		"source_to_DB_EMP_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sg-server.database.windows.net"
		},
		"source_to_DB_EMP_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sg-database"
		},
		"source_to_DB_EMP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sg-admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "copyFactorySG_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"copy_connectionString": {
						"value": "[parameters('copy_connectionString')]"
					},
					"source_to_DB_EMP_password": {
						"value": "[parameters('source_to_DB_EMP_password')]"
					},
					"source_to_DB_EMP_properties_typeProperties_server": {
						"value": "[parameters('source_to_DB_EMP_properties_typeProperties_server')]"
					},
					"source_to_DB_EMP_properties_typeProperties_database": {
						"value": "[parameters('source_to_DB_EMP_properties_typeProperties_database')]"
					},
					"source_to_DB_EMP_properties_typeProperties_userName": {
						"value": "[parameters('source_to_DB_EMP_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}